                  <property name="styleSheet">
                   <string notr="true">/* Стиль для горизонтального QSlider (громкость) как в Spotify */
QSlider {
    min-height: 24px;
    max-height: 24px;
}

/* Фоновая дорожка */
QSlider::groove:horizontal {
    height: 4px;
    background: #535353;
    border-radius: 2px;
}

/* Заполненная часть дорожки */
QSlider::sub-page:horizontal {
    height: 4px;
    background: #1DB954;  /* фирменный зеленый Spotify */
    border-radius: 2px;
}

/* Ползунок (по умолчанию) */
QSlider::handle:horizontal {
    width: 12px;
    height: 12px;
    margin: -4px 0;  /* центрирование по вертикали */
    background: white;
    border: none;
    border-radius: 6px;
}

/* Ползунок при нажатии */
QSlider::handle:horizontal:pressed {
    width: 16px;
    height: 16px;
    margin: -6px 0;
    border-radius: 8px;
    background: #1ED760;  /* светлый зеленый при активации */
}

/* Неактивное состояние */
QSlider:disabled {
    opacity: 0.3;
}

/* Дополнительно: тонкая анимация (если поддерживается) */
QSlider::handle:horizontal {
    transition: all 0.2s ease;
}</string>
